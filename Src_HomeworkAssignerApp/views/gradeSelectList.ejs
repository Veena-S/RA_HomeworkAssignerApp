<!--Grade selection box-->
<%if(dataSubjectGrade.length !== 0 ){%>
<div class="row form-row form-group">
  <label for="grades" class="col-sm-3 col-form-label cust-text-color">Grade:</label>
  <div class="col-sm-8">
    <select name="grade" id="grades">
      <%dataSubjectGrade.forEach((rowGrade) => {%>
        <!--If this form is diplayed as part of editing data, make the item selected-->
    
        <% if((existingData !== undefined) && (existingData !== '')) {%> 
          <% if( rowGrade.grade === existingData.grade ) { %> 
            <option class="cust-border-round" value="<%=rowGrade.grade%>" selected><%= rowGrade.grade%></option>
          <% } else {%> 
            <option class="cust-border-round" value="<%=rowGrade.grade%>"><%= rowGrade.grade %></option>            
          <% } %> 
        <% } else {%> 
          <!--Value in "value" will be submitted. Not name.-->
          <option class="cust-border-round" value="<%=rowGrade.grade%>"><%= rowGrade.grade %></option>
        <% } %> 
      <%});%>
    </select>
  </div>
</div>
<% if(displaySubjectCombo === true) {%> 
  <%- include('subjectSelectList', {distinctSubjectsList: distinctSubjectsList, existingData: existingData}); -%>
<% } %> 
<%}%>
